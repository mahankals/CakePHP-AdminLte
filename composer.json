{
  "name": "mahankals/cakephp-adminlte",
  "description": "CakePHP 5.x plugin for AdminLTE 4 theme",
  "version": "dev-main",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "plugin",
    "adminlte",
    "theme"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Atul Mahankal",
      "email": "atulmahankal@gmail.com",
      "role": "Author"
    }
  ],
  "autoload": {
    "psr-4": {
      "AdminLte\\": "src/"
    }
  },
  "extra": {
    "installer-name": "AdminLte",
    "cakephp": {
      "plugin": "AdminLte"
    }
  },
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "5.1.*",
    "composer/installers": "*"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "scripts": {
    "post-install-cmd":"bin\\cake AdminLte publish_config"
  },
  "support": {
    "issues": "https://github.com/mahankals/CakePHP-AdminLte/issues",
    "source": "https://github.com/mahankals/CakePHP-AdminLte"
  }
}
